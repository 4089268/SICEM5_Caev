@page "/"
@namespace Sicem_Blazor.Pages
@using Microsoft.Extensions.Options
@using Sicem_Blazor.Helpers
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@inject IOptions<BingMapsSettings> bingMapsOptions
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="es-mx">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Sicem - CAEV </title>
    <base href="~/" />
    <link href="_content/Syncfusion.Blazor/styles/bootstrap4.css" rel="stylesheet" />
    <link href="_content/Syncfusion.Blazor/styles/fabric.css" rel="stylesheet" />
    <link href="_content/MatBlazor/dist/matBlazor.css" rel="stylesheet" />
    <link rel="preconnect" href="https://fonts.gstatic.com" />
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@500&display=swap" rel="stylesheet" />
    <link href="~/fonts/Segoe/segoe-ui-font.css" rel="stylesheet" />
    <link href="~/css/site.min.css" rel="stylesheet" />
    <link href="~/css/siteLayout.min.css" rel="stylesheet" />
    <link href="~/css/consultaGeneral.min.css" rel="stylesheet" />
    <link href="~/css/logearsePage.min.css" rel="stylesheet" />
    <link href="~/css/tabs.min.css" rel="stylesheet" />
    <link href="~/css/components/NsComponents.min.css" rel="stylesheet" />

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css" rel="stylesheet" />
    <link href="_content/Blazor.Bootstrap/blazor.bootstrap.css" rel="stylesheet" />

</head>
<body>

    @{
        var connectionInfo = new Sicem_Blazor.Models.ConnectionInfo() {  IpAddress = Request.HttpContext.Connection.RemoteIpAddress.ToString() };
    }

    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" param-ConnectionInfo="connectionInfo" /> />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            Ocurrió un error inesperado. Presione el siguiente botón para recargar la pagina
        </environment>
        <environment include="Development">
            Ocurrió un error inesperado. Presione el siguiente botón para recargar la pagina
        </environment>
        <a href="" class="reload">Recargar</a>
        <a class="dismiss">🗙</a>
    </div>

    
    <script type='text/javascript' src='https://www.bing.com/api/maps/mapcontrol?key=@bingMapsOptions.Value.Key'></script>
    <script src="~/js/JQuery/jquery-3.5.1.min.js"></script>
    <script src="~/js/JQuery/jquery-ui.min.js"></script>
    <script src="~/js/Syncfusion/syncfusion-blazor-base.min.js"></script>
    <script src="https://kit.fontawesome.com/a2a7183415.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.17.20/lodash.min.js"></script>
    <script src="_content/MatBlazor/dist/matBlazor.js"></script>
    <script src="~/js/utilities.js"></script>
    <script src="~/js/animaciones.js"></script>
    <script src="~/js/map.min.js"></script>
    <script src="~/js/facturacion.min.js"></script>
    <script src="_framework/blazor.server.js"></script>
    

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/4.4.1/chart.umd.js" integrity="sha512-ZwR1/gSZM3ai6vCdI+LVF1zSq/5HznD3ZSTk7kajkaj4D292NLuduDCO1c/NT8Id+jE58KYLKT7hXnbtryGmMg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/chartjs-plugin-datalabels/2.2.0/chartjs-plugin-datalabels.min.js" integrity="sha512-JPcRR8yFa8mmCsfrw4TNte1ZvF1e3+1SdGMslZvmrzDYxS69J7J49vkFL8u6u8PlPJK+H3voElBtUCzaXj+6ig==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdn.jsdelivr.net/npm/sortablejs@latest/Sortable.min.js"></script>
    <script src="_content/Blazor.Bootstrap/blazor.bootstrap.js"></script>

</body>
</html>